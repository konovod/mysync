

declare_server(ChessServer, client_sync : ChessTurn, server_sync : ChessBoard, async_commands: {})


class ChessUser < UserContext
  def on_disconnect
  def on_received_static(io : IO)
  def on_sending_static(io : IO)

end


class ChessServer
  def on_login(user : UserID) : UserContext?
  def on_register(user : UserID) : UserContext?
end


